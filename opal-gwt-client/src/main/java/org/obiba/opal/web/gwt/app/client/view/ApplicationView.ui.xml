<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
    xmlns:g="urn:import:com.google.gwt.user.client.ui"
    xmlns:b="urn:import:com.github.gwtbootstrap.client.ui"
    ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
    ui:generateKeys="com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator" ui:generateLocales="default">

  <g:FlowPanel>
    <b:ResponsiveNavbar position="TOP" scrollspy="false" inverse="true">
      <b:Brand href="#">Opal</b:Brand>
      <b:Nav>
        <b:NavLink ui:field="dashboardItem">
          <ui:msg description="Dashboard menu item">Dashboard</ui:msg>
        </b:NavLink>
        <b:NavLink ui:field="datasourcesItem">
          <ui:msg description="Datasources label">Datasources</ui:msg>
        </b:NavLink>
      </b:Nav>

      <b:Nav alignment="RIGHT">
        <b:NavSearch size="2" placeholder="Search"/>
        <b:NavLink ui:field="username">
          <ui:msg description="Administration link text">Administration</ui:msg>
        </b:NavLink>
        <b:NavLink ui:field="administrationItem">
          <ui:msg description="Datasources label">Administration</ui:msg>
        </b:NavLink>
        <b:NavLink ui:field="help">
          <ui:msg description="Help link text">Help</ui:msg>
        </b:NavLink>
        <b:NavLink ui:field="quit">
          <ui:msg description="Quit link text">Quit</ui:msg>
        </b:NavLink>
      </b:Nav>
    </b:ResponsiveNavbar>
    <g:FlowPanel addStyleNames="main">
      <g:FlowPanel ui:field="workbench" addStyleNames="content padding50-top">
        <g:Label>
          <ui:msg description="Workbench label">The Workbench</ui:msg>
        </g:Label>
      </g:FlowPanel>
      <b:FluidRow addStyleNames="footer content padding50-top">
        <b:Column size="6">
          <g:Anchor href="http://obiba.org" target="_blank">Â© 2013 OBiBa</g:Anchor>
        </b:Column>
        <b:Column size="6">
          <g:Label ui:field="version" addStyleNames="pull-right"/>
        </b:Column>
      </b:FluidRow>
    </g:FlowPanel>
    <g:Frame ui:field="frame" visible="false"></g:Frame>
  </g:FlowPanel>
</ui:UiBinder>