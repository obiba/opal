<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
    xmlns:g="urn:import:com.google.gwt.user.client.ui"
    xmlns:c="urn:import:com.google.gwt.user.cellview.client"
    xmlns:o="urn:import:org.obiba.opal.web.gwt.app.client.workbench.view"
    ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
    ui:generateKeys="com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator"
    ui:generateLocales="default">

  <g:FlowPanel ui:field="packagesPanel" addStyleNames="panel">
    <g:VerticalPanel width="100%" addStyleNames="content">
      <g:Button ui:field="addPackageButton" styleName="btn" addStyleNames="bottom-margin">
        <ui:msg description="Add DataShield Package label">Add Package</ui:msg>
      </g:Button>
      <c:SimplePager ui:field="packagesTablePager" location="CENTER" addStyleNames="right-aligned" visible="false"/>
      <o:Table width="100%" ui:field="packagesTable" addStyleNames="gwt-CellTable" visible="false"/>
    </g:VerticalPanel>
  </g:FlowPanel>


</ui:UiBinder>