<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
    xmlns:g="urn:import:com.google.gwt.user.client.ui"
    ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
    ui:generateKeys="com.google.gwt.i18n.server.keygen.MD5KeyGenerator"
    ui:generateLocales="default">

  <ui:style>
    .label {
      margin-left: 1em;
      margin-top: 1em;
    }
  </ui:style>

  <g:FlowPanel>
    <g:Label ui:field="tableListLabel" addStyleNames="{style.label}">
      <ui:msg description="Created/updated table list header">Importing variables to the following tables:</ui:msg>
    </g:Label>
    <g:FlowPanel ui:field="tableListPanel"/>
  </g:FlowPanel>

</ui:UiBinder>