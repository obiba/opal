<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC '//gwt-module/'
    'http://google-web-toolkit.googlecode.com/svn/tags/2.5.1/distro-source/core/src/gwt-module.dtd'>
<module>

  <inherits name="com.google.gwt.user.User"/>
  <inherits name="org.hibernate.validator.HibernateValidator"/>
  <inherits name="org.obiba.opal.web.model.Model"/>

  <replace-with class="org.obiba.opal.web.gwt.app.client.validator.ValidationMessageResolver">
    <when-type-is class="com.google.gwt.validation.client.ProviderValidationMessageResolver"/>
  </replace-with>

</module>
