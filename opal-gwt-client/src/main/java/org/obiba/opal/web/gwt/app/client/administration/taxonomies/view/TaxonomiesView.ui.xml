<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
    xmlns:g="urn:import:com.google.gwt.user.client.ui"
    xmlns:b="urn:import:com.github.gwtbootstrap.client.ui"
    ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
    ui:generateKeys="com.google.gwt.i18n.server.keygen.MD5KeyGenerator"
    ui:generateLocales="default">

  <g:FlowPanel>
    <g:FlowPanel addStyleNames="page-header">
      <b:Breadcrumbs ui:field="breadcrumbs"/>
    </g:FlowPanel>

    <b:FluidRow>
      <b:Column size="3" offset="9">
        <b:ButtonGroup addStyleNames="pull-right">
          <b:Button type="INFO" icon="PLUS" size="SMALL" ui:field="add">
            <ui:msg description="Add Taxonomy label">Add Taxonomy</ui:msg>
          </b:Button>
        </b:ButtonGroup>
      </b:Column>
    </b:FluidRow>
    <g:FlowPanel ui:field="panel" addStyleNames="items"></g:FlowPanel>
  </g:FlowPanel>
</ui:UiBinder>