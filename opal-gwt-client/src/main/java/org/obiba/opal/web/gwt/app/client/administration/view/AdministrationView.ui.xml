                <g:Label styleName="help-block">
                  <ui:msg description="Main Administration SQL Databases help">Manage SQL databases</ui:msg>
                </g:Label>
              </g:FlowPanel>
              <b:Divider/>
              <o:OpalNavLink ui:field="mongoDbPlace" icon="COG">
                <ui:msg description="Main Administration MongoDB title">MongoDB Databases</ui:msg>
              </o:OpalNavLink>
