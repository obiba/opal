<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
    xmlns:g="urn:import:com.google.gwt.user.client.ui"
    xmlns:b="urn:import:com.github.gwtbootstrap.client.ui"
    xmlns:o="urn:import:org.obiba.opal.web.gwt.app.client.ui"
    ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
    ui:generateKeys="com.google.gwt.i18n.server.keygen.MD5KeyGenerator"
    ui:generateLocales="default">

  <g:FlowPanel>
    <g:FlowPanel addStyleNames="main">
      <b:FluidRow>
        <b:Column size="6">
          <g:FlowPanel addStyleNames="extra-large-bottom-margin panel">
            <b:WellNavList>
              <b:NavHeader>
                <ui:msg description="Main Administration Data Access">Data Access</ui:msg>
              </b:NavHeader>
              <b:Divider/>
              <o:OpalNavLink ui:field="usersGroupsPlace" icon="COG">
                <ui:msg description="Main Administration Users and Groups title">Users and Groups</ui:msg>
              </o:OpalNavLink>
              <g:FlowPanel>
                <g:Label styleName="help-block">
                  <ui:msg description="Main Administration Users and Groups help">Add, update, remove users and groups
                  </ui:msg>
                </g:Label>
              </g:FlowPanel>
              <b:Divider/>
              <o:OpalNavLink ui:field="unitsPlace" icon="COG">
                <ui:msg description="Main Administration Organizations title">Organizations</ui:msg>
              </o:OpalNavLink>
              <g:FlowPanel>
                <g:Label styleName="help-block">
